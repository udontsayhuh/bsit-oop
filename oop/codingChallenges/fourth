using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace fourth
{
    internal class Program
    {
        static void Main(string[] args)
        {
            // Enter two numbers
            Console.Write("Enter first value (multiplicand): ");
            int multiplicand = Convert.ToInt32(Console.ReadLine());

            Console.Write("Enter second value (multiplier): ");
            int multiplier = Convert.ToInt32(Console.ReadLine());

            // Increment 1 to print the table until the entered multiplier
            multiplier += 1;

            // Loop for printing the multiplication table
            for (int i = 0; i < multiplier; i++)
            {
                int product = multiplicand * i;
                Console.WriteLine($"{multiplicand} * {i} = {product}");
            }
        }
    }
}
