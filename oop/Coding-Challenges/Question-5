using System;
using System.Linq;
 class Program5
{         
  static void Main (string[] args)    
  {
        
	string[] cars = new string[5];
	
	for (int i = 0; i < 5; i++)
        {
            Console.Write("Enter a car name: ");
            cars[i] = Console.ReadLine().ToUpper();
			
        }
        
    Array.Sort(cars);
	
	Console.WriteLine("\nSorted car names:");
        for (int i = 0; i < 5; i++)
        {
            Console.WriteLine(cars[i]);
}
	    
  }
} 


