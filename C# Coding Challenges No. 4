using System;

class Program
{
    static void Main()
    {
        // Prompt the user to enter the number to be multiplied
        Console.Write("Enter the number to be multiplied: ");
        int number = Convert.ToInt32(Console.ReadLine());

        // Prompt the user to enter the multiplier
        Console.Write("Enter the multiplier: ");
        int multiplier = Convert.ToInt32(Console.ReadLine());

        // Print the multiplication table
        Console.WriteLine($"Multiplication table for {number} up to {multiplier}:\n");

        for (int i = 1; i <= multiplier; i++)
        {
            int result = number * i;
            Console.WriteLine($"{number} x {i} = {result}");
        }
    }
}
