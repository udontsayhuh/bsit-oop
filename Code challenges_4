using System;

class Program
{
    static void Main(string[] args)
    {
        // Prompt user to enter the base number
        Console.WriteLine("Enter the base number:");
        int baseNumber = Convert.ToInt32(Console.ReadLine());

        // Prompt user to enter the multiplier
        Console.WriteLine("Enter the multiplier:");
        int multiplier = Convert.ToInt32(Console.ReadLine());

        // Print multiplication table
        Console.WriteLine($"Multiplication table for {baseNumber} up to {multiplier}:");
        for (int i = 1; i <= multiplier; i++)
        {
            int result = baseNumber * i;
            Console.WriteLine($"{baseNumber} * {i} = {result}");
        }
    }
}
